<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Profile">
    <include
            layout="@layout/layoutmain" tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="66dp"
            tools:ignore="MissingConstraints"
            android:id="@+id/include"/>

    <android.support.v7.widget.CardView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:elevation="12dp"
            android:id="@+id/view2"
            app:cardCornerRadius="40dp"
            android:layout_centerHorizontal="true"
            android:innerRadius="0dp"
            android:shape="ring"
            android:thicknessRatio="1.9"
            tools:layout_editor_absoluteX="56dp"
            tools:ignore="MissingConstraints" android:layout_marginTop="84dp"
            app:layout_constraintTop_toTopOf="@+id/include">
        <ImageView
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:id="@+id/imageView1"
                android:layout_alignParentTop="true"
                android:scaleType="centerCrop"
                android:layout_centerHorizontal="true"
                android:contentDescription="TODO"
                tools:ignore="ContentDescription,HardcodedText" tools:srcCompat="@tools:sample/avatars">
        </ImageView>
    </android.support.v7.widget.CardView>
</android.support.constraint.ConstraintLayout>