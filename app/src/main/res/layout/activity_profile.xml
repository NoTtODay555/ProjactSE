<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Profile">
    <include
            layout="@layout/layoutmain"
            tools:layout_editor_absoluteX="0dp"
            tools:ignore="MissingConstraints" android:id="@+id/include" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/view6" app:layout_constraintVertical_bias="0.51"
            app:layout_constraintVertical_chainStyle="packed"/>

    <View
            android:layout_width="wrap_content"
            android:layout_height="2dp"
            tools:layout_editor_absoluteX="0dp"
            android:background="@color/gray_divider" tools:ignore="MissingConstraints"
            app:layout_constraintTop_toBottomOf="@+id/include"
            android:id="@+id/view6"
    />

    <LinearLayout
            android:background="@drawable/edit_text"
            android:layout_width="match_parent"
            android:layout_height="90dp"
            tools:layout_editor_absoluteX="0dp"
            android:id="@+id/linearLayout"
            android:orientation="horizontal"
            android:gravity="center|start"
            tools:ignore="MissingConstraints,Orientation"
            app:layout_constraintTop_toBottomOf="@+id/view6"
            app:layout_constraintBottom_toTopOf="@+id/view7">


        <de.hdodenhof.circleimageview.CircleImageView
                android:layout_width="84dp"
                android:layout_height="73dp"
                android:id="@+id/circle_proflie"
                tools:ignore="MissingConstraints"
                app:civ_border_color="@color/colorAccent"
                android:layout_marginStart="30dp"
                app:civ_border_width="2dp"
                tools:src="@mipmap/ic_launcher_round"/>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_margin="10dp">

            <TextView
                    android:id="@+id/name_Profile"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Napat Chumnuam"
                    android:fontFamily="@font/changa_one"
                    android:textStyle="bold"
                    android:textColor="@color/browser_actions_text_color"/>

            <TextView
                    android:id="@+id/tel_profile"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="09X - XXX - XXXX"
                    />

            <TextView
                    android:id="@+id/FB_Profile"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="FB : Napat Chumnuam"
                    />

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="0dp"
            android:orientation="vertical"
            android:id="@+id/linearLayout2"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout">


        <LinearLayout
                android:background="@drawable/edit_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                tools:ignore="Orientation"
                android:baselineAligned="false"
                android:fadingEdge="horizontal">

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:gravity="center|start"
                    android:layout_marginStart="20dp"
                    android:textSize="20dp"
                    android:text="Requested Exchange"/>

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="end|center">

                <ImageView
                        android:id="@+id/requested"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_keyboard_arrow_right_black_24dp"
                        android:layout_marginEnd="10dp"/>
            </LinearLayout>
        </LinearLayout>


        <LinearLayout

                android:background="@drawable/edit_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                tools:ignore="Orientation">

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:gravity="center|start"
                    android:layout_marginStart="20dp"
                    android:textSize="20dp"
                    android:text="Panding Exchange"/>

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="end|center">

                <ImageView
                        android:id="@+id/panding"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_keyboard_arrow_right_black_24dp"
                        android:layout_marginEnd="10dp"/>
            </LinearLayout>
        </LinearLayout>


        <LinearLayout

                android:background="@drawable/edit_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                tools:ignore="Orientation">

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:gravity="center|start"
                    android:layout_marginStart="20dp"
                    android:textSize="20dp"
                    android:text="Awaiting Exchange"/>

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="end|center">

                <ImageView
                        android:id="@+id/awaiting"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_keyboard_arrow_right_black_24dp"
                        android:layout_marginEnd="10dp"/>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout

                android:background="@drawable/edit_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                tools:ignore="Orientation">

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:gravity="center|start"
                    android:layout_marginStart="20dp"
                    android:textSize="20dp"
                    android:text="Completed Exchange"/>

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="end|center">

                <ImageView
                        android:id="@+id/completed"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_keyboard_arrow_right_black_24dp"
                        android:layout_marginEnd="10dp"/>
            </LinearLayout>
            <View
                    android:layout_width="wrap_content"
                    android:layout_height="2dp"
                    android:background="@color/gray_divider"/>

        </LinearLayout>
    </LinearLayout>

    <View
            android:layout_width="wrap_content"
            android:layout_height="2dp"
            android:background="@color/gray_divider"
            tools:layout_editor_absoluteX="0dp"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout"
            android:id="@+id/view7"
            />
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="0dp"
            android:orientation="vertical"
            tools:ignore="MissingConstraints" android:id="@+id/linearLayout5"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout2">
        <LinearLayout

                android:background="@drawable/edit_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                tools:ignore="Orientation" android:baselineAligned="false" android:fadingEdge="horizontal">
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:gravity="center|start"
                    android:layout_marginStart="20dp"
                    android:textSize="20dp"
                    android:text="Suggestions"/>
            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="end|center">
                <ImageView
                        android:id="@+id/suggestions"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_keyboard_arrow_right_black_24dp"
                        android:layout_marginEnd="10dp"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
                android:background="@drawable/redsingin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                tools:ignore="Orientation" android:baselineAligned="false" android:fadingEdge="horizontal">
            <TextView
                    android:id="@+id/singOut"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:gravity="center|start"
                    android:layout_marginStart="20dp"
                    android:textSize="20dp"
                    android:text="SIGN OUT"/>
            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="end|center">
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

    <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="0dp"
            tools:ignore="MissingConstraints"
            android:id="@+id/constraintLayout"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/view7">

        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Address"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toTopOf="parent"
                android:id="@+id/textView5"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="24dp"/>

        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Napat chumnuam"
                android:layout_marginTop="32dp"
                app:layout_constraintTop_toBottomOf="@+id/textView5"
                android:id="@+id/textView6"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="24dp"/>

        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ROOM XXXX Drom XXXXX/x"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/textView6"
                android:id="@+id/textView7"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="24dp"/>

        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Phitasanulok - Nakhion 65000"
                app:layout_constraintTop_toBottomOf="@+id/textView7"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="24dp"
                app:layout_constraintBottom_toBottomOf="parent"/>

    </android.support.constraint.ConstraintLayout>


</android.support.constraint.ConstraintLayout>